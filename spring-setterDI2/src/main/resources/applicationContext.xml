<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

	<bean id="address" class="com.meensat.springcore.model.Address">
		<property name="city" value=""></property>
		<property name="state" value=""></property>
		<property name="pin" value='343343'></property>
	</bean>
	<bean id ="student" class="com.meensat.springcore.model.Student">
		<property name="studentId" value='100' ></property>
		<property name="sName" value="Abhi" ></property>
		<property name="address" ref="address"></property>
	</bean>

</beans>