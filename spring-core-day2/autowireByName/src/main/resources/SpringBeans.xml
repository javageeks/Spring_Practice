<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="customer" class="com.greenhorn.spring.model.Customer" autowire="byName" />
		
	<bean id="address" class="com.greenhorn.spring.model.Address" >
		<property name="fulladdress" value="Block A 888, CA" />
	</bean>
<!-- 	<bean id="customer" class="com.greenhorn.spring.model.Customer">
	    <property name="address" ref="address"></property>
	</bean> -->
	
<!--  if the name of a bean is same as the name of other bean property, auto wire it.-->
</beans>